        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_1"
        android:layout_marginTop="@dimen/_160sdp"
        android:layout_marginStart="20dp"
        tools:layout_editor_absoluteX="@dimen/_61sdp"
        tools:layout_editor_absoluteY="@dimen/_204sdp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_160sdp"
        android:text="@string/_2"
        tools:layout_editor_absoluteX="@dimen/_177sdp"
        tools:layout_editor_absoluteY="@dimen/_204sdp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_3"
        android:layout_marginStart="@dimen/_205sdp"
        android:layout_marginTop="@dimen/_160sdp"
        tools:layout_editor_absoluteX="@dimen/_307sdp"
